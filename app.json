{
  "name": "Slot Machine",
  "description": "A slot machine game where the house always wins",
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "env": {
    "RAILS_MASTER_KEY": {
      "description": "Rails master key for credentials",
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql:essential-0"
  ]
}
